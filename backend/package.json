    "express": "^4.21.2",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.20",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.10",
