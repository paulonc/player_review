    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
