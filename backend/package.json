{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "npx prisma migrate dev",
    "test": "mocha -r ts-node/register --require tests/setupTests.ts 'tests/**/*.test.ts'",
    "test:watch": "mocha -r ts-node/register --watch --watch-files src,tests 'tests/**/*.test.ts'",
    "test:coverage": "nyc mocha --require ts-node/register --recursive tests/**/*.test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
